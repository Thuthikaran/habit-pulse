



<%= simple_form_for(@habit) do |f| %>
  <%= f.input :name %>
  <%= f.input :priority %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :reminder %>
  <%= f.input :frequency %>
  <%= f.input :status %>
  <%= f.input :category %>
  <%= f.input :days_of_week %>

  <%= f.submit %>
<% end %>
